{
  "name": "battle-bots",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "set:electron": "mv ./src/background.ts.nobuild ./src/background.ts; VUE_CLI_SERVICE_CONFIG_PATH= ",
    "set:web": "mv ./src/background.ts ./src/background.ts.nobuild; VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.config.js",
    "serve": "npm run set:web; vue-cli-service serve",
    "build": "npm run set:web; vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "electron:build": "npm run set:electron; vue-cli-service electron:build",
    "electron:serve": "npm run set:electron; vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "jsmpeg": "^1.0.0",
    "moment": "^2.27.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-vue": "^0.11.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.5",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "electron": "^9.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "typescript": "~3.9.3",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.4",
    "vue-template-compiler": "^2.6.11"
  }
}
