{
  "name": "gpeapi",
  "version": "0.0.1",
  "description": "",
  "repository": "",
  "original": "https://github.com/vrudikov/typescript-rest-boilerplate.git",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build:bundle": "rm -rf dist && yarn run build",
    "start:cluster": "cross-env ENV=prod && yarn && yarn run swagger && cross-env NODE_OPTIONS=--max-old-space-size=800 && cross-env NODE_ENV=production ts-node src/cluster.ts",
    "docker:build": "rm -rf dist && yarn run build && cp Dockerfile dist && cd dist && docker build -t battlebots-api .",
    "build": "yarn run tsc && yarn run swagger && cp swagger.yaml dist && cp -r config dist && cp -r src/public dist/src && cp ormconfig.prod.json dist/ormconfig.json && cp package.json dist && cd dist && yarn",
    "clean": "rm -rf dist",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./test/**/*.ts -t verbose",
    "pretest": "cross-env NODE_ENV=test yarn run build",
    "test": "cross-env NODE_ENV=test ts-mocha 'test/**/*.ts'",
    "test:coverage": "nyc yarn run test",
    "swagger": "swaggerGen -c ./swaggerConfig.json",
    "tsc": "tsc",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts",
    "typeorm:migration:create": "yarn run typeorm:cli -- migration:create -n ",
    "typeorm:migration:run:production": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:run -c production",
    "typeorm:migration:revert:production": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:revert -c production",
    "typeorm:migration:run:staging": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:run -c staging",
    "typeorm:migration:revert:staging": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:revert -c staging"
  },
  "dependencies": {
    "@mojotech/json-type-validation": "^3.1.0",
    "aws-sdk": "^2.814.0",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.8",
    "check-ip": "^1.1.1",
    "class-validator": "^0.11.0",
    "concurrently": "^5.1.0",
    "config": "^3.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "gulp-typescript": "^6.0.0-alpha.1",
    "html": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "morgan": "1.9.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "sinon": "^9.0.0",
    "tree-kill": "^1.2.1",
    "typeorm": "^0.2.18",
    "typescript-ioc": "^3.2.1",
    "typescript-rest": "^2.2.0",
    "typescript-rest-swagger": "^1.1.1",
    "uuidv4": "^6.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "4.1.7",
    "@types/config": "0.0.36",
    "@types/cors": "2.8.4",
    "@types/dotenv": "6.1.1",
    "@types/expect": "^24.3.0",
    "@types/express": "4.16.0",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.5",
    "@types/moment": "^2.13.0",
    "@types/morgan": "1.7.35",
    "@types/node": "^12.6.2",
    "@types/passport-http-bearer": "^1.0.35",
    "@types/passport-jwt": "^3.0.1",
    "@types/request": "2.48.1",
    "@types/sinon": "^7.5.1",
    "chai": "4.2.0",
    "cross-env": "5.2.0",
    "gulp": "^4.0.2",
    "gulp-mocha": "^7.0.2",
    "mocha": "5.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "rimraf": "2.6.3",
    "source-map-support": "0.5.9",
    "ts-mocha": "^6.0.0",
    "ts-node": "7.0.1",
    "tslint": "5.17.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "3.5.1"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "report-dir": "./reports/coverage",
    "sourceMap": true,
    "instrument": true
  }
}
